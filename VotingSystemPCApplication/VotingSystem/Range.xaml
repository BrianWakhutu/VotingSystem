<Window x:Class="VotingSystem.Range"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VotingSystem"
        mc:Ignorable="d"
        Title="Range" Height="167.038" Width="181.277" AllowsTransparency="true" WindowStartupLocation="CenterScreen" Background="{x:Null}" WindowStyle="None">
    <Grid>
        <Calendar x:Name="Start" HorizontalAlignment="Left" SelectedDatesChanged="UserStartDate" FirstDayOfWeek="Monday" Margin="0,0,0,-6" VerticalAlignment="Top" BorderBrush="Transparent" SelectionMode="SingleDate" Background="{StaticResource LightBackground}" MouseDoubleClick="Close"/>
    </Grid>
</Window>
